htenment to commentaries on popular culture, high art, commodity fetishism, and mass society. This introduction to the programmatic statements and eclectic reflections of various scholars will highlight the diverse historical influences, collaborative efforts, and internecine debates that shaped the intellectual tradition across continents and generations.","catalogBreadth":null,"catalogDistr":null,"catalogLang":null,"catalogForbiddenOverlaps":null,"catalogWhenOffered":null,"catalogComments":null,"catalogPrereqCoreq":null,"catalogFee":null,"catalogSatisfiesReq":null,"catalogPermission":null,"catalogCourseSubfield":null,"catalogOutcomes":null,"acadCareer":"UG","acadGroup":"AS"},{"strm":2608,"crseId":367386,"crseOfferNbr":3,"subject":"ENGL","catalogNbr":"3931","titleShort":"The Refusal of Work","titleLong":"The Refusal of Work","enrollGroups":[{"classSections":[{"ssrComponent":"LEC","ssrComponentLong":"Lecture","section":"001","classNbr":17859,"meetings":[{"classMtgNbr":1,"timeStart":"01:25PM","timeEnd":"02:40PM","startDt":"08\/25\/2015","endDt":"12\/04\/2015","instructors":[{"instrAssignSeq":1,"netid":"bhm4","firstName":"Barry","middleName":"Hamilton","lastName":"Maxwell"}],"pattern":"TR","facilityDescr":"To Be Assigned","bldgDescr":null,"facilityDescrshort":null,"meetingTopicDescription":""}],"notes":[],"campus":"MAIN","campusDescr":"Ithaca","location":"ITH","locationDescr":"Ithaca, NY (Main Campus)","startDt":"08\/25\/2015","endDt":"12\/04\/2015","addConsent":"N","addConsentDescr":"No Special Consent Required","isComponentGraded":true,"instructionMode":"P","instrModeDescrshort":"In Person","instrModeDescr":"In Person","topicDescription":""}],"unitsMinimum":4,"unitsMaximum":4,"componentsOptional":[],"componentsRequired":["LEC"],"gradingBasis":"OPT","gradingBasisShort":"Stdnt Opt","gradingBasisLong":"Student Option","simpleCombinations":[{"subject":"AMST","catalogNbr":"3731","type":"C"},{"subject":"COML","catalogNbr":"3731","type":"C"}],"sessionCode":"1","sessionBegi